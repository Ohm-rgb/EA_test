# ===================================
# AI Trading OS - Environment Config
# ===================================

# Database
DATABASE_URL=sqlite:///./dev.db

# JWT Authentication
JWT_SECRET=change-this-to-a-secure-random-string
JWT_EXPIRE_HOURS=24

# MT5 Connection
MT5_SERVER=
MT5_LOGIN=
MT5_PASSWORD=

# AI - Local (Ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:8b

# AI - External (Gemini)
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash

# Redis (for caching)
REDIS_URL=redis://localhost:6379

# App Settings
APP_ENV=development
DEBUG=true
CORS_ORIGINS=http://localhost:3000
